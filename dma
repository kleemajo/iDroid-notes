format
s5l8900:
	uart0 8e1b0000 00000000 2400c03c 2000c13c 07 000000000000000000000000000000
			07/10 -> 0E/10
	uart3 1a100000 00000000 24c0c03c 00000000 00 000000000000000000000000000000
			0D/10 -> -
		  000b0000 00000000 00000000 20c0c03c 06 000000000000000000000000000000
			-     -> 0C/10 
	uart4 9e190000 00000000 2400c13c 2000c03c 00 000000000000000000000000000000
			0F/10 -> 06/10
	flash 84080000 00904800 8000a038 8000a038 00 000000000000000000000000000000
			02/10 -> 02/10
	spi2  800b0000 00900800 00000000 1000203d 03 000000000000000000000000000000
			-	  -> 10/0E
	i2s1  84080000 00902400 00000000 1000d03c 01 000000000000000000000000000000
			- 	  -> 10/02
		  c6100000 00902400 3800d03c 00000000 01 000000000000000000000000000000
			10/03 -> -
	i2s0  00080000 00902400 00000000 1000a03c 00 000000000000000000000000000000
			-	  -> 00/00
		  42100000 00902400 3800a03c 00000000 00 000000000000000000000000000000
			01/01 -> -
	uart1 12100000 00000000 2440c03c 00000000 00 000000000000000000000000000000
			09/09 -> -
	mbx	  c0010000 00904800 00000000 0000803b 05 000000000000000000000000000000
			
                            frm-addr to-addr   


